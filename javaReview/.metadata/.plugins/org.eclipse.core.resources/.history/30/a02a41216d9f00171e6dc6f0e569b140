package rensyu;

import java.util.Scanner;

public class rensyu1 {

	public static void main(String[] args) {
	 //switch문을 이용하여 봄여름가을겨울 판단하기 
		
		int getsu =0;
		char kisetsu= ' ';
		
		System.out.println("몇 월인가 ?");
		
		Scanner scan= new Scanner(System.in);
		String input= scan.nextLine(); //몇월인지 넣는  변수
		getsu = Integer.parseInt(input); // int형으로 변환
		
		switch(getsu){
		case 3:
		case 4:
		case 5:
			System.out.println("현재 계절은 봄입니다");
		    break;
		case 6:
		case 7:
		case 8:
			System.out.println("현재 계절은 여름입니다.");		
		    break;
		    
		case 9:
		case 10:
		case 11:
			System.out.println("현재 계절은 가을입니다.");		
		    break;
		case 12:
		case 1:
		case 2:
			System.out.println("현재 계절은 겨울입니다.");		
		    break;
			    
		    
		    
		}
	}

}
